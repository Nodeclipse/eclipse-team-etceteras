<fileset-config file-format-version="1.2.0" simple-config="true">
  <local-check-config name="checkstyle-configuration" location=".checkstyle-config.xml" type="project" description="CheckStyle configuration">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="checkstyle-configuration" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
</fileset-config>
